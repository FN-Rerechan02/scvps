RUN=yes
DAEMON=/usr/sbin/sslh
DAEMON_OPTS="--user sslh --listen 0.0.0.0:2443 --ssh 127.0.0.1:22 --tls 127.0.0.1:1013 --socks5 127.0.0.1:1080 --http 127.0.0.1:700 --pidfile /var/run/sslh/sslh.pid -n"